package com.cg.iservice;

import java.util.List;

import com.cg.entity.DeliveryAgent;
import com.cg.exception.ResourceNotFoundException;

public interface IDeliveryAgentService {
	DeliveryAgent createAgent(DeliveryAgent agent);
	 
    DeliveryAgent updateAgent(Integer id, DeliveryAgent agent) throws ResourceNotFoundException;
 
    DeliveryAgent getAgentById(Integer id) throws ResourceNotFoundException;
 
    List<DeliveryAgent> getAllAgents();
 
    void deleteAgent(Integer id) throws ResourceNotFoundException;
}
